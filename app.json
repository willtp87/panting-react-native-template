{
  "expo": {
    "name": "Panting React Native Template",
    "slug": "panting-react-native-template",
    "scheme": "panting-scheme",
    "newArchEnabled": true,
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ]
  }
}
